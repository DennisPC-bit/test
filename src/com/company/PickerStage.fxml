<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<AnchorPane fx:id="ap" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.PickerStageController">
   <children>
      <BorderPane fx:id="root" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" minHeight="0" minWidth="0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <left>
            <Button fx:id="addContentBtn" mnemonicParsing="false" onAction="#addContent" text="Button" BorderPane.alignment="CENTER_RIGHT" />
         </left>
         <center>
            <Button mnemonicParsing="false" onAction="#splitSceneHorizontally" text="Hor" BorderPane.alignment="CENTER" />
         </center>
         <right>
            <Button mnemonicParsing="false" onAction="#splitSceneVertically" text="Vert" BorderPane.alignment="CENTER_LEFT" />
         </right>
      </BorderPane>
   </children>
</AnchorPane>
